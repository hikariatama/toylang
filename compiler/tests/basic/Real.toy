method Main is
    var a : Real
    a := 5.4
    IO.PrintLine(a)
    a := Real(6.66)
    IO.PrintLine(a)
    var b : Real(5.93)
    IO.PrintLine(b)
    b := a.Plus(b)
    IO.PrintLine(b)
    a := b.Plus(23.44)
    IO.PrintLine(a)
    a := a.Plus(Real(5.5))
    IO.PrintLine(a)
    a := a.Minus(3.333)
    IO.PrintLine(a)
    a := a.Mult(2.1)
    IO.PrintLine(a)
    IO.PrintLine(a.Equal(1))
    IO.PrintLine(a.LessThan(2))
    IO.PrintLine(a.GreaterThan(0))
    IO.PrintLine(a.LessEqual(1))
    IO.PrintLine(a.GreaterEqual(2))
    IO.PrintLine(a.NotEqual(1))
    IO.PrintLine(Real(0).Plus(0.0000000001))
    IO.PrintLine(Real(1).Div(3))
    IO.PrintLine(Real(-3.5).UnaryMinus())
    IO.PrintLine(Real(7.5).Mod(Real(2.25)))
    IO.PrintLine(Real(7.5).Mod(2))
    IO.PrintLine(Real(5.5).ToInteger())
end
