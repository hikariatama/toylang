method Main is
    var colors1 : Array[String](Integer(2))
    colors1.set(0, "red")
    colors1.set(1, "blue")
    var colors2 : Array[String](Integer(3))
    colors2.set(0, "green")
    colors2.set(1, "yellow")
    colors2.set(2, "purple")

    var palette : List[Array[String]]()
    palette.append(colors1)
    palette.append(colors2)
    IO.PrintLine(palette)

    var palettes : List[List[Array[String]]]()
    palettes.append(palette)
    IO.PrintLine(palettes)

    var complexMap : Map[Real, List[Array[String]]]()
    complexMap.set(2.5, palette)
    var minimal : List[Array[String]]()
    minimal.append(colors1)
    complexMap.set(7.75, minimal)
    IO.PrintLine(complexMap.get(2.5))
    IO.PrintLine(complexMap.get(7.75))

    var listOfMaps : List[Map[String, Real]]()
    var pointA : Map[String, Real]()
    pointA.set("x", 1.5)
    pointA.set("y", 2.5)
    var pointB : Map[String, Real]()
    pointB.set("x", -3.0)
    pointB.set("y", 4.0)
    listOfMaps.append(pointA)
    listOfMaps.append(pointB)
    IO.PrintLine(listOfMaps)

    var arrayOfLists : Array[List[Integer]](Integer(2))
    var seq1 : List[Integer](1, 2, 3)
    var seq2 : List[Integer](8, 13, 21, 34)
    arrayOfLists.set(0, seq1)
    arrayOfLists.set(1, seq2)
    IO.PrintLine(arrayOfLists.get(0))
    IO.PrintLine(arrayOfLists.get(1))

    var nestedMap : Map[String, Map[Integer, List[String]]]()
    var teamMembers : Map[Integer, List[String]]()
    var devs : List[String]("alice", "bob")
    var ops : List[String]("carol")
    teamMembers.set(1, devs)
    teamMembers.set(2, ops)
    nestedMap.set("teams", teamMembers)
    IO.PrintLine(nestedMap.get("teams").get(1))
    IO.PrintLine(nestedMap.get("teams").get(2))
end
