method Main is
    var a : Integer
    a := 5
    IO.PrintLine(a)
    a := Integer(6)
    IO.PrintLine(a)
    var b : Integer(5)
    IO.PrintLine(b)
    b := a.Plus(b)
    IO.PrintLine(b)
    a := b.Plus(5)
    IO.PrintLine(a)
    a := a.Plus(Integer(5))
    IO.PrintLine(a)
    a := a.Minus(3)
    IO.PrintLine(a)
    a := a.Mult(2)
    IO.PrintLine(a)
    IO.PrintLine(a.Equal(1))
    IO.PrintLine(a.LessThan(2))
    IO.PrintLine(a.GreaterThan(0))
    IO.PrintLine(a.LessEqual(1))
    IO.PrintLine(a.GreaterEqual(2))
    IO.PrintLine(a.NotEqual(1))
    IO.PrintLine(Integer(7).Div(2))
    IO.PrintLine(Integer(9).Div(2.0))
    IO.PrintLine(Integer(7).Mod(3))
    IO.PrintLine(Integer(7).Mod(2.5))
    IO.PrintLine(Integer(7).Rem(3))
    IO.PrintLine(Integer(7).UnaryMinus())
    IO.PrintLine(Integer(0).ToBoolean())
    IO.PrintLine(Integer(5).ToBoolean())
    IO.PrintLine(Integer(3).ToReal())
    IO.PrintLine(Integer(3).ToInteger())
end
