method Evaluate(a : Integer, b : Integer, c : Integer) : List[Integer] is
    var results : List[Integer]()
    results.append(a.Plus(b))
    results.append(a.Minus(b))
    results.append(a.Mult(c))
    results.append(a.Div(c))
    results.append(a.Mod(c))
    results.append(a.Rem(Integer(4)))
    return results
end

method Main is
    var sequence : List[Integer]()
    sequence := Evaluate(Integer(20), Integer(7), Integer(3))
    IO.PrintLine(sequence)
    IO.PrintLine(Integer(5).UnaryMinus())
    IO.PrintLine(Integer(10).LessThan(5))
    IO.PrintLine(Integer(10).GreaterEqual(10))
    IO.PrintLine(Integer(10).Equal(10))
    IO.PrintLine(Integer(10).NotEqual(9))
    IO.PrintLine(Integer(10).Div(4.0))
    IO.PrintLine(Integer(10).Mod(2.5))
    IO.PrintLine(Integer(10).Rem(3))
end
